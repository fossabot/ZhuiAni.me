{
  "name": "zhui-anime-webui",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --ignore-unknown --write 'src/**/*'",
    "build-api": "swagger-typescript-api -p http://localhost:5000/api/swagger/v1/swagger.json -t build/api-template -o src -n api.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/styled-system": "^2.9.1",
    "@chakra-ui/system": "^2.6.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "escape-string-regexp": "^5.0.0",
    "framer-motion": "^10.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.10.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.2",
    "swr": "^2.2.0",
    "usehooks-ts": "^2.9.1",
    "whatwg-mimetype": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/whatwg-mimetype": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "swagger-typescript-api": "^13.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-eslint": "^1.8.1"
  },
  "prettier": {
    "printWidth": 120
  }
}
