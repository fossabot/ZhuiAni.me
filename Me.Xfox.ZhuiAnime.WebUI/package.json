    "autoprefixer": "^10.4.14",
    "daisyui": "^2.51.4",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.11.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
