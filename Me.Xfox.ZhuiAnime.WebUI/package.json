{
  "name": "zhui-anime-webui",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
