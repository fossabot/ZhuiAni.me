    "autoprefixer": "^10.4.14",
    "daisyui": "^2.51.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.12.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "swagger-typescript-api": "^12.0.4",
